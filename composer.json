{
  "name": "mashytski/trumail",
  "description": "Laravel package to enable email validation through trumail.io API's",
  "version": "1.0.0-dev",
  "keywords": ["Mashytski","Laravel","Trumail"],
  "homepage": "https://gitlab.com/mashytski",
  "license": "MIT",
  "authors": [
    {
      "name": "Kelvin Macharia",
      "email": "ngunyimacharia@gmail.com",
      "homepage":"https://gitlab.com/mashytski",
      "role": "Developer"
    }
  ],

  "support": {
    "email": "ngunyimacharia@gmail.com",
    "issues": "https://gitlab.com/mashytski/laravel-trumail/issues"
  },

  "require": {
      "php" : "^7.0"
  },

  "require-dev":{
    "orchestra/testbench": "^3.4",
    "phpunit/phpunit": "^7.2",
    "fzaninotto/faker": "^1.7"
  },

  "autoload": {
    "psr-4": {
      "Mashytski\\Trumail\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Mashytski\\Trumail\\Tests\\": "tests/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "Mashytski\\Trumail\\TrumailServiceProvider"
      ]
    },
    "aliases": {
      "Trumail": "Mashytski\\Trumail\\Facades\\Trumail"
    }
  }

}
